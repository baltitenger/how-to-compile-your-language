// RUN: compiler %s -ast-dump 2>&1 | filecheck %s
struct Empty {}
// CHECK: StructDecl: Empty

struct WithTrailingComma {
    r: number,
    g: number,
    b: number,
}
// CHECK: StructDecl: WithTrailingComma
// CHECK-NEXT:   MemberDecl: r:number
// CHECK-NEXT:   MemberDecl: g:number
// CHECK-NEXT:   MemberDecl: b:number

struct WithoutTrailingComma {
    a: number,
    b: number,
    c: number
}
// CHECK: StructDecl: WithoutTrailingComma
// CHECK-NEXT:   MemberDecl: a:number
// CHECK-NEXT:   MemberDecl: b:number
// CHECK-NEXT:   MemberDecl: c:number
